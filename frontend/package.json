  "private": true,
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.4.0",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "format": "prettier --write src/**/*.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "start": "PORT=3030 react-scripts start",
